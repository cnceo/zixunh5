body{
	background: #ffffff;
	overflow-y: hidden;
}
.headinfo{
	height: 36px;
	color: #d5aa9b;
	font-size:.14rem;
	line-height: .36rem;
	padding-left:.36rem;
	text-align: left;
	border-bottom: 1px solid #D9D9D9;
	background: #fbf2eb url(//static.letoula.com/images/v1.0/info/award/announce.png) no-repeat 13px center;
}
.jczq_items{
	height:100%;
	overflow-y: auto;
}
.wraper{
	padding-top: 0.44rem;
	padding-bottom:.72rem;
	height:100%;
	overflow-x:hidden ;
	overflow-y:auto ;
}
.no_padding_wraper{
	padding-bottom:0
}
.jczq_date div{
	box-sizing: border-box;
	background: #F4F4F4;
	width:100%;
	font-size:.16rem;
	height:.36rem;
	line-height: .36rem;
	border-bottom: 1px solid #D9D9D9;
}
.jczq_date{
	box-sizing: border-box;
	text-indent: .13rem;
	background: #F4F4F4;
	font-size:15px;
	color: #7F7F7F;
	height: .36rem;
	line-height: .36rem;
	text-align: left;
}
.jczq_item{
	position: relative;
	height: 1.3rem;
	box-sizing: border-box;
	padding: 0 0.2rem 0 .1rem;
	border-bottom: 1px solid #D9D9D9;
}
.am-arrow-right{
    width:.2rem;
    height: 100%;
    position: absolute;
    right:0;
    top:0;
    background: url(/Static/images/v1.0/info/award/arrow_r.png) no-repeat center center;
    background-size: 30%;
}
.jczq_item_title{
	height: .4rem;
	line-height: 0.4rem;
	font-size:.2rem;
	color: #383838;
	overflow: hidden;
}
.jczq_ls span{
	height: .2rem;
	line-height: .2rem;
	display: block;
	font-size:.12rem;
	color: #ACACAC;
}
.jczq_ls span:nth-child(1){
		line-height: .26rem;
}
.jczq_ls{
	width:18%;
	float:left;
	text-align: left;
	box-sizing: border-box;
}
.jczq_item_tt{
	width:82%;
	float:left;	
	height:.4rem;
	text-align: center;
	font-size:.15rem;
	color:#383838;
}
.jczq_item_title label{
	display: inline-block;
	height: .4rem;
	font-size:.15rem;
	margin:0 .2rem;
	font-weight: bolder;
}
.jczq_item_result table tr:nth-child(1) td{
		width: 20%;
		height:.3rem;
		box-sizing: border-box;
		border:1px solid #D9D9D9;
		font-size:.12rem;
}
.jczq_item_result table tr:nth-child(2) td{
		width: 20%;
		height:.48rem;
		box-sizing: border-box;
		border:1px solid #D9D9D9;
		font-size:.14rem;
		font-weight: bolder;
}
.jczq_item_result table tr:nth-child(2) td span{
		font-size:.1rem;
}
.jczq_item_result table td.title span{
		display: inline-block;
		font-size:.12rem;
}

.hc-h5-wrap>footer::before {
    content: "";
    display: block;
    position: absolute;
    width: 100%;
    height: 0px;
    overflow: hidden;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.3);
}
.hc-h5-wrap>footer {
    width: 100%;
    height: 0.7rem;
    line-height: 0.7rem;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 99;
    border-top:2px solid #D9D9D9;
    background-color:#ffffff; 
    padding:0.1rem 0.13rem;
}


footer .footer_btn {
	width:100%;
	height:100%;
	background-color:#E12D3E ;
	border-radius: 0.03rem;
	position: relative;
}
footer button.btn_red {
	position: absolute;
	top:50%;
	width:100%;
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
	transform: translateY(-50%);
	font-family: "微软雅黑";
	font-size: .19rem;
	background-color:#E12D3E ;
	width: 100%;
	border: 0;
	color: #FFFFFF;
	border-radius: 3px;
	vertical-align: middle;
	outline: none !important;
	cursor: pointer;
}

.jczq_item_result table tr td{
	text-align: center;
}
/**********单天*************/

/**********日期**********/

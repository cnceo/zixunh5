/*完全赛程*/
/*分组的*/
.tab2 .item_header_{
    background-position: 0.10rem bottom,0.45rem 0.10rem;
}
.data_match{
    border-top: 1px solid #000913;
    border-bottom: 1px solid #00295b;
}
.date_title{
    height:0.34rem;
    line-height: 0.34rem;
    color: #e9c60a;
    font-size: 0.14rem;
    padding: 0 0.10rem
}
.item_main2 .match_wrap{
   width: 100%;
   height:0.51rem;
   line-height: 0.51rem;
    background-image: url(/Static/images/v1.0/info/zixun/item_bg2.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    color: #fff;
    padding-right: 0.08rem;
    font-size: 0.14rem
}
.item_main2 .match_wrap>span{
    display: inline-block;
    float: left;
    height: 0.51rem;
    line-height: 0.51rem;
    text-align: center;
}
.item_main2 .match_wrap>span:nth-child(1){
    width: 14.2%;
}
.item_main2 .match_wrap>span:nth-child(2){
    width: 14.9%;
    border-right: 1px solid transparent;
    -webkit-border-image: url(/Static/images/v1.0/info/zixun/split_2.png) 1 round;
    -o-border-image: url(/Static/images/v1.0/info/zixun/split_2.png) 1 round;
    border-image: url(/Static/images/v1.0/info/zixun/split_2.png) 1 stretch;
}
.item_main2 .match_wrap>span:nth-child(3){
    width: 17.5%
}
.item_main2 .match_wrap>span:nth-child(4){
    width: 16%
}
.item_main2 .match_wrap>span:nth-child(5){
    width: 5.2%
}
.item_main2 .match_wrap>span:nth-child(6){
    width: 16%
}
.item_main2 .match_wrap>span:nth-child(7){
    width: 16.2%
}
.item_main2 .match_wrap>span .group,.zu{
    display: inline-block;
    text-shadow: 2px 2px 2px #002865;
    transform: skew(-8deg);
    -ms-transform: skew(-8deg); /* IE 9 */
    -webkit-transform: skew(-8deg); /* Safari and Chrome */
}
.item_main2 .group_{
    font-size: 0
}
.item_main2 .group{
    display: inline-block;
    font-weight: bolder;
    font-size:0.21rem;
    color: #fff;
    
}
.item_main2 .zu{
    display: inline-block;
    font-size: 0.13rem;
    color: #ffd800;
    line-height: 0.54rem;
    padding-left: 0.02rem
}
.tab2 .match_wrap .hostName{
    text-align: left
}
.tab2 .match_wrap .guestName{
    text-align: right
}
.hostFlag,.guestFlag{
    display: inline-block;
    width: 0.43rem;
    height: 0.27rem;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
}

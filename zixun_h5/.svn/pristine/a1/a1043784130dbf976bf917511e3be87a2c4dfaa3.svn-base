/*今日好彩公共样式*/
body{
    font-family: "Source Han Sans CN","Microsoft YaHei","Helvetica",simsun
}
.hc-h5-wrap>header{
    background-color: #FD8237 ;
}
.hc-h5-wrap .header-right {
    color: #fff;
}
.hc-h5-wrap .header-title {
    /* text-align: left; */
    width: 0.92rem;
    background: url(/Static/images/v1.0/info/zixun/logo2.png) no-repeat center;
    background-size: contain;
}
/*今日好彩公共样式*/
/*img {
    width: 100%;
    height: 100%
}*/

.hc-h5-wrap {
    padding-top: 0.44rem
}

.wraper {
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: relative;
    background-color: #fff
}

.content_wrap {
    width: 100%;
    height: 100%;
    background-image: url("/Static/images/v1.0/info/zixun/worldCup_bg.jpg");
    background-repeat: no-repeat;
    background-size: 100% 100%
}

.content {
    width: 100%;
    height: 100%;
}

.content_inner {
    width: 100%;
    height: 100%;
    overflow: hidden;
    overflow-y: auto;
    position: relative;
    padding-bottom: 0.10rem
}

/*32强阅兵*/
.item_header{
    background-image: url("/Static/images/v1.0/info/zixun/worldCup_logo_3.png"),url("/Static/images/v1.0/info/zixun/worldCup_logo_2.png");
    background-repeat: no-repeat;
    background-size: 0.25rem 0.50rem,0.62rem 0.15rem;
    background-position: 0.10rem bottom,0.45rem 0.25rem;
    margin-bottom: 0.12rem
}
.item_bg{
    height: 0.44rem;
    width: 100%;
    background-image: url("/Static/images/v1.0/info/zixun/worldCup_head_bg.png");
    background-repeat: no-repeat;
    background-size: 100% 100%

}
.item_title{
    height: 0.25rem;
    line-height: 1;
    text-indent: 0.45rem;
    color: #d8f5f6
}
.item_main,.item_main2,.item_main3{
    width: 100%;
    padding: 0.06rem 0.10rem;
}
.item_main .item{
    width: 48%;
    background-image: url("/Static/images/v1.0/info/zixun/item_bg12.png");
    background-repeat: no-repeat;
    background-size: 100% 100%
}
.item_main .item:first-child{
    float: left;
}
.item_main .item:last-child{
    float: right;
}
.inner_title{
    height: 0.38rem;
    line-height: 0.38rem;
    text-align: center;
    background-image: url("/Static/images/v1.0/info/zixun/item_bg11.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.inner_title span{
    display: inline-block;
    text-shadow: 2px 2px 2px #002865;
    transform: skew(-8deg);
    -ms-transform: skew(-8deg); /* IE 9 */
    -webkit-transform: skew(-8deg); /* Safari and Chrome */

}
.inner_title .group{
    font-weight: bolder;
    font-size:0.21rem;
    color: #fff;
}
.inner_title .zu{
    font-size: 0.13rem;
    color: #ffd800;
    padding-left: 0.02rem;
}

.match_item{
    font-size: 0.14rem;
    padding: 0.09rem 0;
    padding-left: 0.16rem;
    height: 0.53rem;
    border-bottom: 1px solid transparent;
    -webkit-border-image: url('/Static/images/v1.0/info/zixun/split_1.png') 30 round; /* Safari 3.1-5 */
    -o-border-image: url('/Static/images/v1.0/info/zixun/split_1.png') 30 round;; /* Opera 11-12.1 */
    border-image: url('/Static/images/v1.0/info/zixun/split_1.png') 30 round;;
}
.match_item span{
    display: inline-block;
    height: 0.35rem;
    float: left;
}  
.match_item span{
    height: 0.35rem;
    line-height: 0.35rem
}
.match_item .country_flag{
    margin-right:0.17rem;
    width: 0.54rem;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
}  
.match_item .country_name{
    
    color: #fff
} 

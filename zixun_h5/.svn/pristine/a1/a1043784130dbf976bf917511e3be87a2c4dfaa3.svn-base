}
.out_title,.out_wrap_title {
    height: 0.34rem;
    line-height: 0.34rem;
    color: #e9c60a;
    font-size: 0.14rem;
    padding: 0 0.10rem;
}
.out_wrap{
    padding:0.10rem 0;
    border-top: 1px solid #000913;
    border-bottom: 1px solid #00295b;
}
.out_wrap_title{
    height: 0.25rem;
    font-size: 0.15rem;
    text-align: center;
    transform: skew(-8deg);
    -ms-transform: skew(-8deg);
    -webkit-transform: skew(-8deg);
}













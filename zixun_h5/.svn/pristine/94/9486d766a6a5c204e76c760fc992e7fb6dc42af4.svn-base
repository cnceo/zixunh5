html,body{
	min-width: 320px;
	width: 100%;
	height: 100%;
}
body{
	box-sizing: border-box;
	font: 0.13rem/1.5 "Helvetica", "Microsoft YaHei", simsun;
	background-color: #f4f4f4;
	color: #383838;	
}
.spinner {
  margin: auto;
  width: 0.6rem;
  text-align: center;
}
.spinner > i {
  width: 0.16rem;
  height: 0.16rem;
  margin: 0.02rem;
  background-color: #FFFFFF;
  border-radius: 100%;
  display: inline-block;
  -webkit-animation: bouncedelay 1.4s infinite ease-in-out;
  animation: bouncedelay 1.4s infinite ease-in-out;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
.spinner .bounce1 {
  -webkit-animation-delay: -0.5s;
  animation-delay: -0.5s;
}
.spinner .bounce2 {
  -webkit-animation-delay: -0.25s;
  animation-delay: -0.25s;
}
@-webkit-keyframes bouncedelay {
  0%, 80%, 100% { -webkit-transform: scale(0.0) }
  40% { -webkit-transform: scale(1.0) }
}
@keyframes bouncedelay {
  0%, 80%, 100% {
    transform: scale(0.0);
    -webkit-transform: scale(0.0);
  } 40% {
    transform: scale(1.0);
    -webkit-transform: scale(1.0);
  }
}
/*header start*/
.hc-h5-wrap{
	width: 100%;
	height: 100%;
}
.hc-h5-wrap>header,
.hc-h5-wrap>footer{
	background: -webkit-linear-gradient(left,#FF1E00, #FFC100) !important; /* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(left,#FF1E00, #FFC100) !important; /* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(left,#FF1E00, #FFC100) !important; /* Firefox 3.6 - 15 */
	background: linear-gradient(left,#FF1E00, #FFC100) !important; /* 标准的语法 */	
	min-width: 320px;
}

.hc-h5-wrap>header{
	width: 100%;
	height: 0.44rem;
	line-height: 0.44rem;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99999;
}
.hc-h5-wrap>footer{
	width: 100%;
	height: 0.5rem;
	line-height: 0.5rem;
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 99;
}
.hc-h5-wrap>header::before,
.hc-h5-wrap>footer::before{
  content: "";
  display: block;
  position: absolute;
  width: 100%;
  height: 0px;
  overflow: hidden;
  left: 0;
  right: 0;
  background: rgba(0,0,0,0.3);
}
.hc-h5-wrap>header::before{
  bottom: 0;
}
.hc-h5-wrap>footer::before{
  top: 0;
}
.hc-h5-wrap .header-left,
.hc-h5-wrap .header-right{
	height: 100%;
	position: absolute;
	top: 0;
	overflow: hidden;
	clear: both;
}
.hc-h5-wrap .header-left{
	left: 0;
	width: 0.44rem;
}
.hc-h5-wrap .header-right{
	right: 0;
	width: 0.44rem;
}
.hc-h5-wrap .header-title{
	height: 100%;
	margin: 0 0.44rem 0 0.44rem;
	font-size: 0.19rem;
	font-weight: 500;
	color: #fff;	
	text-align: center;
	overflow: hidden;
}
.header-icon,
.back,
.filter,
.menu,
.help,
.calen{
	width: 0.44rem;
	height: 100%;
	position: relative;
	display: block;  
}
.header-icon::before,
.back::before,
.filter::before,
.menu::before,
.help::before,
.calen::before{
	content: "";
	display: block;
	width: 0.24rem;
	height: 0.24rem;
	position: absolute;
	top: 0;
	bottom: 0;
	right: 0;
	left: 0;
	margin: auto;
}
.back::before{
	background: url(/Static/images/v1.0/back.png) no-repeat center center;
	background-size: 0.22rem;	
}
.filter::before{
	background: url(/Static/images/v1.0/filter.png) no-repeat center center;
	background-size: 0.22rem;	
}
.menu::before{
	background: url(/Static/images/v1.0/menu.png) no-repeat center center;
	background-size: 0.22rem;	
}
.help::before{
  background: url(/Static/images/v1.0/help.png) no-repeat center center;
  background-size: 0.22rem; 
}
.calen::before{
  background: url(/Static/images/v1.0/calendar.png) no-repeat center center;
  background-size: 0.22rem; 
}
.header-drop{
	display: inline-block;
	line-height: 0.28rem;
	padding-right: 0.14rem;
	position: relative;
	white-space: nowrap;
}
.header-drop::before{
	content: "";
	width: 0.1rem;
	height: 0.1rem;
	position: absolute;
	right: 0;
	bottom: 0.06rem;
	background: url(/Static/images/v1.0/drop.png) no-repeat right bottom;
	background-size: 0.1rem;
}
/*header end*/

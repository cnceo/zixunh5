/*pull down*/
#refresh{
	width: 100%;
	height: auto;
	position: absolute;
	top: 0;
	left: 0;
	transform: translateY(-100%);
  -webkit-transform: translateY(-100%);
}

.pull_logo{
	background: url(/Static/images/v1.0/trade/sports/jczq/pulldown.gif) no-repeat center bottom;
	background-size: 1.28rem 0.5rem;
	height: 0.5rem;
}


.blink::after{
    content:'';
    padding-right: 1px;
    border-right: 1px blue solid;
    -webkit-animation: blink 1s infinite;
    animation: blink 1s infinite;
}
.mul_input.active::after{
    content:'';
    padding-right: 2px;
    border-right: 1px blue solid;
    -webkit-animation: blink 1s infinite;
    animation: blink 1s infinite;
}
@keyframes blink{
  from
    {
      border-right:1px #fff solid;
    }
  to
    {
      border-right:1px blue solid;
    }
}
@-webkit-keyframes blink{
  from
    {
     border-right:1px #fff solid;
    }
  to
    {
      border-right:1px blue solid;
    }
}

/* loading */
.hc-loding{
  width: 100%;
  height: 100%;
}
/****.hc-loding .status配置距离高度*****/
.hc-loding .status{
  width: 100%;
  height: 0.24rem;
  left: 0;
  right: 0;
  margin: auto;
  position: relative;
  overflow: visible;
}
.hc-loding p{
  width: 100%;
  font-size: 0.16rem;
  line-height: 0.2rem;
  text-align: center;
  position: absolute;
  top: 0.35rem;
  color: #7f7f7f;
  margin: 0;
}
.hc-loding .status:before{
  content: "";
  width: 1.5rem;
  height: 0.08rem;
  top: 0.08rem;
  left: 0;
  right: 0;
  margin: auto;
  position: absolute;
  background: url("/Static/images/v1.0/common/loading_bg.png") no-repeat;
  background-size: 1.5rem 0.08rem;
  background-position: center center;
}
.hc-loding .status:after{
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 1.74rem;
  height: 0.24rem;
  background: url("/Static/images/v1.0/common/loading.png") no-repeat;
  background-size: 0.24rem 0.24rem;
  background-position: center left;
  -webkit-background-position: center left;
  animation:load 1.2s infinite linear ;
    -webkit-animation:load 1.2s infinite linear ; /*Safari and Chrome*/
}
@keyframes load{
  0% {
    background-position: center left;
    -webkit-background-position: center left;
  }
  100% {
    background-position: center right;
    -webkit-background-position: center right;
  }
}
@-webkit-keyframes load{
  0% {
    background-position: center left;
    -webkit-background-position: center left;
  }
  100% {
    background-position: center right;
    -webkit-background-position: center right;
  }
}

/*success fail modal*/
.modal-icon{
  width: .8rem;
  height: .8rem;
  margin: 0 auto .1rem;
  background-repeat: no-repeat;
  background-position: center center;
  -webkit-background-size: contain;
  background-size: contain;
}
.modal-icon-fail{
  background-image: url('/Static/images/v1.0/common/fail.png');
}
.modal-icon-success{
  background-image: url('/Static/images/v1.0/common/success.png');
}
.modal-close-btn{
  position: absolute;
  top: .06rem;
  right: .06rem;
  z-index: 1;
  font-size: 0;
  display: inline-block;
  width: .16rem;
  height: .16rem;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}
.modal-close-btn::before,
.modal-close-btn::after{
  display: block;
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  border-color: #383838;
  border-style: solid;
  border-width: 0;
}
.modal-close-btn::before{   
    width: 0.16rem;
    height: 0;
    border-top-width: 2px;
}
.modal-close-btn::after{
    height: 0.16rem;
    width: 0;
    border-right-width: 2px;  
}

















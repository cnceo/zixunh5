  height: 0.24rem;
  line-height: 0.24rem;
  margin: 0.1rem;
  font-size: 0.12rem;
}
.head-icon{
  display: inline-block;
  width: 0.24rem;
  height: 0.24rem;
  margin: 0.1rem;
  background-size: 0.22rem;
  background-repeat: no-repeat;
  background-position: center center;
}
.head-icon-back{
  background-image: url(/Static/images/v1.0/back.png);
}
.head-icon-refresh{
  background-image: url(/Static/images/v1.0/refresh.png);
}
.head-icon-filter{
  background-image: url(/Static/images/v1.0/filter.png);
}
.head-icon-menu{
  background-image: url(/Static/images/v1.0/menu.png);
}
.head-icon-help{
  background-image: url(/Static/images/v1.0/help.png);
}
.head-icon-setting{
  background-image: url(/Static/images/v1.0/setting.png);
}
.head-icon-calen{
  background-image: url(/Static/images/v1.0/help.png);
}
.hc-head-drop{
  display: inline-block;
  max-width: 100%;
  line-height: 0.28rem;
  padding-right: 0.14rem;
  position: relative;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;

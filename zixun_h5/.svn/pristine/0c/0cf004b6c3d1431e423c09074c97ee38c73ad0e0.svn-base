						$(this).attr("src", "/Static/images/v1.0/worldcup/lx_" + index + "" + index + "" + ".png?v=23e432423")
					}
					switch(index) {
						case 0:
							setTimeout(function() {
								window.location.href = "lx_jqcx.html";
							}, 200)
							break;
						case 1: //有福同享

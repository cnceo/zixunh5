		/*幻灯片播放图片 css*/
		.swiper-container {
		    margin-left: auto;
		    margin-right: auto;
		    position: relative;
		    overflow: hidden;
		    z-index: 1;
		    height: 100%;
		}
		.swiper-container-android .swiper-slide, .swiper-wrapper {
		    -webkit-transform: translate3d(0,0,0);
		    -moz-transform: translate3d(0,0,0);
		    -o-transform: translate(0,0);
		    -ms-transform: translate3d(0,0,0);
		    transform: translate3d(0,0,0);
		}
		.swiper-wrapper {
		    position: relative;
		    width: 100%;
		    height: 100%;
		    z-index: 1;
		    display: -webkit-box;
		    display: -moz-box;
		    display: -ms-flexbox;
		    display: -webkit-flex;
		    display: flex;
		    -webkit-transition-property: -webkit-transform;
		    -moz-transition-property: -moz-transform;
		    -o-transition-property: -o-transform;
		    -ms-transition-property: -ms-transform;
		    transition-property: transform;
		    -webkit-box-sizing: content-box;
		    -moz-box-sizing: content-box;
		    box-sizing: content-box;
		    -webkit-align-items: center;
			align-items: center;
		}
		.swiper-slide {
		    -webkit-flex-shrink: 0;
		    -ms-flex: 0 0 auto;
		    flex-shrink: 0;
		    width: 100%;
		    position: relative;
		}
		.swiper-container-horizontal > .swiper-scrollbar {
		    position: absolute;
		    left: 1%;
		    bottom: 3px;
		    z-index: 50;
		    height: 5px;
		    width: 98%;
		}
		.image-check{
			position: fixed;
			width: 100%;
			height: 100%;
			/*min-height: */
			left: 0;
			right: 0;
			top: 0;
			bottom: 0;
			margin: auto;
			overflow: hidden;
			background-color: rgb(0,0,0);
			z-index: 19941116
		}
		.image-content{
			width: 100%;
			/*min-height: 50%;*/
			height: 100%;
			text-align: center;
		}
		.swiper-pagination{
			position: absolute;
			font-size: 20px;
			line-height: 1;
			bottom: 22px;
			left: 15px;
			color: #fff;
			z-index: 66;
			
		}
		.swiper-pagination-current{
			font-size: 30px;
			line-height: 0.8;
			display: inline-block;
			vertical-align: bottom;
			/*font-weight: 600;*/
		}
		.swiper-pagination-total{
			display: inline-block;
			vertical-align: bottom;
			line-height: 0.8;
			font-weight: 200;
		}
		.swiper-pagination-dis{
			display: inline-block;
			vertical-align: bottom;
			font-weight: 100
		}

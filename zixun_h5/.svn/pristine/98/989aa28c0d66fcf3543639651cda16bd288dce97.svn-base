/* 返回键的层级最高 */
.hc-head-left{
	z-index: 19941117
}
.back_gradient{
	background: -webkit-linear-gradient(left,#FF1E00, #FFC100) !important; /* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(left,#FF1E00, #FFC100) !important; /* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(left,#FF1E00, #FFC100) !important; /* Firefox 3.6 - 15 */
	background: linear-gradient(left,#FF1E00, #FFC100) !important; /* 标准的语法 */
}
.header-drop {
    display: inline-block;
    line-height: 0.28rem;
    padding-right: 0.14rem;
    position: relative;
    white-space: nowrap;
}
/* 玩法选择 */
.hc-nav {
	position: fixed;
	left: 0;
	top: 0.40rem;
	background-color: #FFF;
	display: none;
	padding-top: 0.08rem;
	padding-bottom: 0.08rem;

	z-index: 97;
	-webkit-box-shadow:0 2px 0.10rem 0 #666;
    box-shadow:0 2px 0.10rem 0 #666;
}
.hc-btn{
	width: 100%;
	height: 0.34rem;
	line-height: 0.34rem;
    color: #7F7F7F;
    border: 1px solid #d9d9d9;
    border-radius: 0.03rem;
    color: #7f7f7f;
    font-size: 0.13rem;
    padding: 0
}
.hc-btn.hc-btn-nav-selected{
	border-color: #FD8237;
	color: #FD8237;
	-webkit-box-shadow: 0 0 0 1px #FD8237 inset;
    box-shadow: 0 0 0 1px #FD8237 inset;
	background-image: url(/Static/images/v1.0/live/choosed.png);
	background-repeat: no-repeat;
	background-position: right bottom;
	-webkit-background-size: 0.15rem;
	background-size: 0.15rem;
}

/* mobile彩种选择 */
.m_header-wrap{
	width: 100%;
	height: 0.35rem;
}
.m_header{

	height: 0.35rem;
	position: fixed;
	top: 0;
	background-color: #fff;
	z-index: 99;
	border-bottom: 1px solid #d9d9d9
}
.m_main{
	width: 100%;
	height: 100%;
	padding: 0.08rem 0.4rem 0.08rem 0;
	position: relative;
}
.m_lotylist{
	width: 100%;
	height: 100%;
	overflow-x: auto;
	overflow-y: hidden;
}
.m_lotylist>ul{

/***/
body{
	background: #ffffff;
	overflow: hidden;
	height:100%;
}

.wraper{
	padding-bottom:.72rem;
	padding-top:.44rem;
	height:100%;
	overflow-y: auto;
}
.no_padding_wraper{
	padding-bottom:0
}
.wraper .qlc_items{
	height:100%;
	overflow-y: auto;
}

/**底部按钮**/
.hc-h5-wrap>footer::before {
    content: "";
    display: block;
    position: absolute;
    width: 100%;
    height: 0px;
    overflow: hidden;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.3);
}
.hc-h5-wrap>footer {
    width: 100%;
    height: 0.7rem;
    line-height: 0.7rem;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 99;
    border-top:2px solid #D9D9D9;
    background-color:#ffffff; 
    padding:0.1rem 0.13rem;
}
footer .footer_btn {
	width:100%;
	height:100%;
	background-color:#E12D3E ;
	border-radius: 0.03rem;
	position: relative;
}
footer button.btn_red {
	position: absolute;
	top:50%;
	width:100%;
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
	transform: translateY(-50%);
	font-family: "微软雅黑";
	font-size: .19rem;
	background-color:#E12D3E ;
	width: 100%;
	border: 0;
	color: #FFFFFF;
	border-radius: 3px;
	vertical-align: middle;
	outline: none !important;
	cursor: pointer;
}

.qlc_items ul li.latest{
	height: .94rem;
	background: #FFF8F1;
	margin: 0;
	padding: 0 .1rem;
	box-sizing: border-box;
}

.arrow_r{
	display: inline-block;
	float: right;
	width: .2rem;
	height: 100%;
	background: url(//static.letoula.com/images/v1.0/info/award/arrow_r.png) no-repeat right center;
}
.qlc_items ul li{
	box-sizing: border-box;
	margin: 0 .08rem;
	height: .88rem;
	border-bottom: 1px solid #D9D9D9;
	text-align: left;
	background: url(//static.letoula.com/images/v1.0/info/award/arrow_r.png) no-repeat right center;
}
.qlc_items ul li.latest p{
	height: .44rem;
	line-height: .44rem;
	padding-left:2px;
}
.qlc_items ul li.latest .qlc_item{
	height: .5rem;
	line-height: .5rem;
	margin-top:-5px;
}
.qlc_items ul li.latest .qlc_item span,
.qlc_header .qlc_item span{
	display: inline-block;
	width: .36rem;
	height: .36rem;
	border-radius: .18rem;
	line-height: .36rem;
	background: #E12D3E;
	color: #FFFFFF;
	text-align: center;
	font-size:.16rem;
	font-weight: 500;
	margin: 0 2px;
	margin-top:-2px;
}
.qlc_items ul li.latest .qlc_item span:nth-child(1){
	margin-left: 0px;
}
.qlc_items ul li p{
	padding: 0px;
	margin: 0px;
	height: .44rem;
	line-height: .44rem;
	font-size:.13rem;
	color: #383838;
}
.qlc_items ul li p span.issue{
	font-family: 微软雅黑
}
.qlc_items ul .latest p span.issue{
	font-weight: bold;
}
.qlc_items ul li .qlc_item{
	margin-top:-.05rem;
	text-indent:.1rem;
}
.qlc_items ul li.latest .qlc_item{
	margin-top:-.05rem;
	text-indent:0px
}
.qlc_item span{
	color:#E12D3E;
	font-size:.16rem;
	font-weight: bold;
	margin: 0 .1rem;
}
.qlc_item span:nth-child(1){
	margin-left:0px;
}
.qlc_item span:last-child{
	color:#1C88F7;
	font-size:.16rem;
}
span.qlc_money{
	display: block;
	font-size: .14rem;
	color: #E12D3E;
	font-weight: bold;
}
.qlc_items ul li p span.date,
.qlc_header p span.date{
	color: #ACACAC;
	margin: 0 10px;
}
.qlc_item{
	height: .40rem;
	line-height: .40rem;
}
.qlc_items ul li.latest p span{
	font-size:.13rem;
}
.latest_sign{
	display: inline-block;
	background: #ff850e;
	border-radius: .03rem;
	padding: 0 0.03rem;
	text-align: center;
	line-height: .14rem;
	color: #fff;
	box-sizing: border-box;
	font-size:.12rem !important;
}
.qlc_items ul li.latest .qlc_item span:last-child,
.qlc_header .qlc_item span:last-child{
	background: #1C88F7;
}

/*************详情*************/
.qlc_header{
	margin:0;
	padding: 0 .1rem;
	text-align: left;
	background: #FFFFFF;
}
.qlc_header .ssq_kjx_top{
	height:.94rem;
	border-bottom: 1px solid #D9D9D9;
}
.qlc_header p{
	margin: 0;
	padding:0;
	height: .44rem;
	line-height: .44rem;
	font-size:.14rem;
}
.qlc_header p span.issue{
	font-family: 微软雅黑;
	font-weight: bold;
}
.qlc_header .qlc_item{
	height: .5rem;
}
.qlc_header .qlc_item span:last-child{
	color:#ffffff;
}

.qlc_detail_content{
	width: 100%;
	height: auto;
	background: #FFFFFF;
	margin: .12rem 0;
}
.qlc_detail_content table{
	width: 100%;
	border-collapse: collapse;
	font-size:.14rem;
	color: #383838;
}
.qlc_detail_content table td{
	border:1px solid #D9D9D9;
	height: 35px;
	text-align: center;
	box-sizing: border-box;
}
.qlc_detail_content table td.bonus{
	color: #E12D3E;
}
.qlc_detail_content table td.title{
	color: #7F7F7F;
}
.qlc_detail_content table tr td:first-child{
	border-left:0;
}
.qlc_detail_content table tr td:last-child{
	border-right:0;
}
.xlgc{
	height: .8rem;
	padding: .15rem 0;
	box-sizing: border-box;
}
.qlc_xl,
.qlc_gc{
	width: 50%;
	float: left;
	display: inline-block;
	padding-left:.05rem;
	box-sizing: border-box;
}
.qlc_xl span,
.qlc_gc span{
	font-size:.12rem;
}
span.qlc_money{
	font-size:.15rem;
	color: #E12D3E;
	font-weight: bold;
}
.hc-padding{
	padding: .1rem !important;
	color: #acacac;
}
/* 开奖查询优化 */
.kj_issue_search{
	width: 100%;
	height: .46rem;
	padding: .06rem .1rem;
	position: relative;
	z-index: 9999;
}
.kj_issue_search .search-issue{
	display: block;
	width: 100%;
	height: 100%;
	line-height: .34rem;
	background-color: #e7e8ea;
	color: #919191;
	border: 0;
	border-radius: .05rem;
	text-indent: .1rem;
	font-size: .15rem;
	
}
.kj_issue_search .search-issue.add-search-logo{
	color: #000;
	padding-left: 42%;
	background:#e7e8ea url(//static.letoula.com/images/v1.0/info/award/search-logo.png) no-repeat 40% center;
	background-size: .15rem .15rem;
}
.issue-search-close{
	width: .3rem;
	height: 100%;
	display: block;
	position: absolute;
	right: .1rem;
	top: 0px;
	background: url(//static.letoula.com/images/v1.0/info/award/search-start.png) no-repeat  center;
	background-size: 50%;
}
.issue-search-close.bg-change{
	background: url(//static.letoula.com/images/v1.0/info/award/search-issue.png) no-repeat  center;
	background-size: 50%;
}
/* 键盘 */
.inner_val{
	font-style: normal;
	background-color: #78a7e6;
}
.keyboard{
  width: 100%;
  background-color: #fff;
  border-collapse: collapse;
  table-layout: fixed;
  text-align: center;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1111;
}
.keyboard td{
  border: 1px solid #D9D9D9;
}
.keyboard thead td{
  height: 0.5rem;
  text-align: center;
  color: #383838;
  font-size: 0.18rem;
  font-weight: normal;
}
.keyboard thead td p{
	font-size: 0.13rem;
	line-height: 0.16rem;
	color: #59a7e0;
}
.keyboard thead td.active{
    color: #ffb10b;
}
.keyboard tbody td{
  height: 0.62rem;
  width: 33.3333333%;
  color: #383838;
  font-size: 0.32rem;
}
.keyboard thead td:empty{
	height: 0;
	border: 0; 
}
.keyboard tbody .sure{
  color: #fff;
  font-size: .22rem;
  background-color: #e12d3e;
}
.keyboard tbody .del{
  font-size: 0;
  background-image: url('//static.letoula.com/images/v1.0/trade/sports/zqdc/del.png');
  background-repeat: no-repeat;
  background-position: center center;
  -webkit-background-size: 0.35rem 0.23rem;
  background-size: 0.35rem 0.23rem;
  background-color: #f0f0f0;
}
.keyboard tbody .ctrl_key:active{
	background-color:rgba(0,0,0,0.2)
}
.del_line:active{
	background-color:rgba(0,0,0,0.2)
}
/*****************简单兼容5*********/

@media (max-width: 320px) {
	.qlc_items ul li.latest .qlc_item span, .qlc_header .qlc_item span {
   		width:30px;
   		height: 30px;
   		line-height: 30px;
    }
}
*{margin: 0;padding: 0;}
/*滚动水平导航栏 start*/			
.lr_nb {
	height: 0.4rem;
	width: 100%;
	position: relative;
	background: #fff;
	box-sizing: border-box;
	z-index: 2;
	opacity: 1;
	top: 0;
	-webkit-transition: .3s all;
	transition: .3s all;
	float: left;margin-bottom: 5px;
	box-shadow: 1px 1px 15px rgba(229,229,229,0.8);
}
.slider_wrap .line{
	height: 0.4rem;
	line-height: 0.4rem;
}
.wx_items{
	height: 0.4rem;
}
.lr_nb .slider_wrap.line {
	overflow-x: scroll;
	overflow-y: hidden;
	width: 100%;
}

.lr_nb .slider_wrap.line .item_cell {
	display: inline-block;
	margin: 0px 0.08rem;		
	position: relative;
	text-align: center;
}
.lr_nb .slider_wrap.line .item_cell:nth-child(1){
	margin-left: 0.15rem;
}
.lr_nb .slider_wrap.box {
	overflow: hidden;
	width: 100%
}

.lr_nb .slider_wrap::-webkit-scrollbar {
	display: none
}

.lr_nb .wx_items {
	white-space: nowrap
}

.lr_nb .wx_items span {
	color: #333;
	font-size: 0.14rem;
	white-space: nowrap;
	display: block;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	text-align: center;
	cursor: pointer;
	margin-top: -2px;
}
.lr_nb .wx_items p {
	color: #333;
	font-size: 0.13rem;text-align: center;
	height: 0.4rem;
    margin: 0;
    line-height: 0.4rem;
}

.lr_nb .wx_items .current span,
.lr_nb .wx_items .current p,
.lr_nb .wx_items .current a:visited,
.lr_nb .wx_items .current a:link,
.lr_nb .wx_items .current a:hover,
.lr_nb .wx_items .current a:focus {
	color: #fd8237;
	font-size: 0.15rem;
}

.lr_nb_after {
	height: 0.4rem;
	display: block;
	clear: both;
}
.tab_border {
	height: 2px;width: 30px;margin-left: 0.15rem;
	background: #FD8237;
	margin-top: -2px;
}
/*滚动水平导航栏 end*/
/*旋转*/
.rodte{
	transform: rotate(-90deg);
	transition: all 0.3s;
}
.rodte-back{
	transform: rotate(-0deg);
	transition: all 0.3s;
}
/*右上角弹出菜单选项*/
.tab-menu{position: fixed;top: 0.44rem;height: 100%;width: 100%;z-index: 11;background: rgba(0,0,0,0);display: none;}
.tab-warp{position: absolute;top: 0;right: 0.16rem;width: 1.1rem;height: 2rem;background: #FFFFFF;z-index: 12;box-shadow: 1px 1px 15px #e5e5e5;}
.menu-i{width: 100%;height: 0.4rem;line-height: 0.4rem;border-bottom: 1px solid rgba(229,229,229,0.5);}	
.menu-i p{float: right;right: 0.2rem;position: relative;color: #333;font-size: 0.14rem;}
.menu-i span{
	display: inline-block;height: 0.26rem;margin-left: 0.2rem;margin-top: 0.07rem;		
	background-size:100%;background-repeat:no-repeat ;
}
.tab-warp .menu-i:nth-child(1) span{
	width: 0.21rem;background-image: url(/Static/images/v1.0/match/m_yingc.png);
}
.tab-warp .menu-i:nth-child(2) span{
	width: 0.26rem;background-image: url(/Static/images/v1.0/match/m_fajia.png);margin-left: 0.175rem;
}
.tab-warp .menu-i:nth-child(3) span{
	width: 0.32rem;background-image: url(/Static/images/v1.0/match/m_yijia.png);margin-left: 0.145rem;
}
.tab-warp .menu-i:nth-child(4) span{
	width: 0.225rem;background-image: url(/Static/images/v1.0/match/m_dejia.png);margin-left: 0.1925rem;
}
.tab-warp .menu-i:nth-child(5) span{
	width: 0.265rem;background-image: url(/Static/images/v1.0/match/m_xijia.png);margin-left: 0.1725rem;
}

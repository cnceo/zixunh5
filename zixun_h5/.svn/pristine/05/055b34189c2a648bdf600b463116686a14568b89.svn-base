* {margin: 0;padding: 0;}
.container {width: 100%;height: 520px;background: #ccc;position: relative;overflow: hidden;overflow-y: scroll;}
.item-c{position: relative;width: 100%;background: #FFFFFF;}
.date-i{background: #e9e9e9;width: 100%;position: relative;z-index: 2;height: 0.28rem;line-height: 0.28rem;text-align: center;font-size: 0.11rem;color: #333;}
.body-c{width: 100%;position: relative;z-index: 1;}
.fixed-yes{position: fixed;}
.fixed-no{position: relative;}
.list-i{border-bottom: 1px solid #E9E9E9;height: 1.2rem;}
.c-left{
	text-align: center;
}
.c-left p,.c-right p{margin: 0;font-size: 0.13rem;color: #333333;}
.c-left img,.c-right img{width: 0.56rem;height: 0.56rem;border-radius: 50%;border: 1px solid  #e9e9e9;margin-top: 0.25rem;margin-bottom: 0.02rem;}
.c-right{
	text-align: center;
}
.c-center{text-align: center;}
.c-center p{color: #666;font-size: 0.11rem;line-height: 1;margin: 0;}
.c-center h2{font-size: 0.26rem;font-weight: bold;color: #000;line-height: 1;margin: 0;}
.c-center img{width: 0.26rem;height: 0.26rem;margin: 0.09rem 0;}
.hd-content{width: 100%;padding: 0.15rem 0.15rem 0 0.15rem;}
.hd-item{width: 100%;height: 1.74rem;border-radius: 6px;margin-bottom: 0.15rem;background-size:cover;background-position: center;background-repeat:no-repeat ;display: block;}
.nodata-center{padding: 0.74rem 0;}
.nodata-center img{width: 1.33rem;height: 1.065rem;margin-bottom: 0.1rem;}

.wraper ,.content_wrap,.content,.content_inner,.container{height: auto;}
.mui-bar-nav~.mui-content {
    padding-bottom: 0.55rem;
}
.content_inner{
	padding-top: 0;
}
.s_back_content{background-repeat: no-repeat;background-size:cover ;background-position:center ;}
#refresh {
     transform: translateY(-100%); 
    -webkit-transform: translateY(-100%);padding-bottom: 10px;padding-top: 15px;
    background-color: transparent;
}
.match-type{
	width: 100%;background: #E9E9E9;font-size: 0.11rem;color: #999;line-height: 0.15rem;
    border-bottom-left-radius: 4px;border-bottom-right-radius: 4px;
}
.refresh-right{
	display: none;
	position: fixed;bottom: 1.2rem;right: 0.05rem;width: 0.44rem;height: 0.44rem;border-radius: 6px;z-index: 9;
	background: -webkit-linear-gradient(left,#ff1e00, #ffc100); /* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(right,#ff1e00, #ffc100); /* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(right,#ff1e00, #ffc100); /* Firefox 3.6 - 15 */
	background: linear-gradient(to right,#ff1e00, #ffc100);
}
.refresh-right img{position: absolute;top: 0;bottom: 0;left: 0;right: 0;margin: auto;width: 0.255rem;height: 0.22rem;}

.not-start-warp{
	position: fixed;top: 0;height: 100%;width: 100%;background: rgba(0,0,0,0.5);z-index: 999;
}
.inner-warp{width: 2.65rem;height: 1.4rem;background: #FFFFFF;position: absolute;top: 0;bottom: 0;left: 0;right: 0;margin: auto;border-radius: 5px;}
.inner-warp h4{width:100%;position: relative;text-align: center;font-size:16px;color:rgba(42,42,42,1);line-height:1;font-weight: 500;margin-top: -0.2rem;}
.i-header{width: 1.64rem;height: 1.64rem;margin-top: -0.75rem;margin-left: 0.505rem;}
.i-close{width: 0.13rem;height: 0.13rem;position: absolute;top: 0.05rem;right: 0.05rem;}

/*iPhoneX 等全屏幕手机 样式*/
@media only screen and (min-width: 375px) and (min-height: 812px){
	.nodata-center{padding: 1.5rem 0;}
}




/* 加载动画*/

.loading {
	/*width: 100%;
	height: 100%;
	position: fixed;
	left: 50%;
	top: 0;
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-ms-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	padding-top: 3rem;
	background-color: #fff;
	z-index: 99;*/
	
	height: 100%;
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 100;
	background-color: #fff;
	
}

.loading_bar {
	width: 30%;
	height: 0.10rem;
	background: url("/Static/images/v1.0/loading_bg.png") no-repeat center;
	background-size: 100% 100%;
	position: absolute;
	left: 50%;
	top: 1.2rem;
	-webkit-transform: translate(-50%, 0);
	-moz-transform: translate(-50%, 0);
	-ms-transform: translate(-50%, 0);
	transform: translate(-50%, 0);
}

.loading_ball {
	width: 0.30rem;
	height: 0.30rem;
	background: url("/Static/images/v1.0/loading.png") no-repeat center;
	background-size: 100% 100%;
	position: absolute;
	left: 30%;
	top: 1.08rem;
	animation: myfirst 2s;
	-webkit-animation: myfirst 2s;
	/* Safari and Chrome */
	animation-timing-function: linear;
	-webkit-animation-timing-function: linear;
	animation-iteration-count: infinite;
	-webkit-animation-iteration-count: infinite;
	/*Safari and Chrome*/
}

.shuju {
	width: 100%;
	text-align: center;
	position: absolute;
	top: 40%;
}

@keyframes myfirst {
	from {
		left: 30%;
	}
	to {
		left: 60%;
	}
}

@-webkit-keyframes myfirst
/* Safari and Chrome */

{
	from {
		left: 30%;
	}
	to {
		left: 60%;
	}
}



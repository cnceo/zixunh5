/*焦点赛事*/

#focusEvent {
	box-sizing: border-box;
	width: 100%;
	min-height: 1.2rem;
	border-bottom: 1px solid #f0f2f5;
	padding: 0.21rem 0;
	font-size: 0.15rem;
	position: relative;
}

.swiper-focusEvent {
	position: relative;
}

#focusEvent .focusCenter {
	padding-left: 0.12rem;
	/*新修改可以考虑不用*/
	width: 3.1rem;
	margin: 0 auto;
	height: 100%;
	padding-right: 0.12rem;
}

#focusEvent .bottomBall {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}

#focusEvent .bottomBall div {
	height: 0.31rem;
	width: 0.31rem;
	border-radius: 50%;
	text-align: center;
	line-height: 0.30rem;
}

#focusEvent .bottomBall .redBall {
	border: 1px solid #FF1E00;
	color: #FF1E00;
}

#focusEvent .bottomBall .blueBall {
	border: 1px solid #3797FA;
	color: #3797FA;
}

#focusEvent .focusCenter .topMess {
	overflow: hidden;
	margin-bottom: 0.06rem;
}

#focusEvent .focusCenter .topMess .lotyName,
#focusEvent .focusCenter .topMess .lotyMoney {
	height: 0.45rem;
	float: left;
}

#focusEvent .focusCenter .topMess .lotyName{
	width: 46%;
}
#focusEvent .focusCenter .topMess .lotyMoney {
	position: relative;
	width: 54%;
	display: flex;
	flex-direction: row;
	justify-content: flex-end;
}

#focusEvent .focusCenter .topMess .lotyName span {
	color: #1a1a1a;
	font-size: 0.1rem;
}

#focusEvent .focusCenter .topMess .lotyName span:nth-child(1) {
	display: block;
	font-size: 0.15rem !important;
	color: #000000 !important;
	margin-bottom: 0.01rem;
}
#focusEvent .focusCenter .topMess .lotyName span:nth-child(2),
#focusEvent .focusCenter .topMess .lotyName span:nth-child(3){
	position: relative;
	top: -0.05rem;
	color: gray;
}


#focusEvent .focusCenter .topMess .lotyMoney div {
	/*
	display: block;
	position: absolute;
	top: -0.05rem;*/
	float: right;
}

#focusEvent .focusCenter .topMess .lotyMoney div:nth-child(1) {
	width: 0.85rem;
	height: 0.45rem;
	font-size: 0.38rem;
	color: #FD8237 ;
	line-height: 0.45rem;
	letter-spacing: -4px;
	text-align: center ;
	margin-right: 0.05rem;
}
#focusEvent .focusCenter .topMess .lotyMoney .tempClass{
	letter-spacing: 0px !important;
	text-align: right !important;

}
#focusEvent .focusCenter .topMess .lotyMoney div:nth-child(2) {
	box-sizing: border-box;

/*新增条数提示*/

.hasUpdata {
	height: 0.35rem;
	line-height: 0.35rem;
	text-align: center;
	font-size: 0.14rem;
	color: #6391db;
	opacity: 0;
	background-color: #e6ebf0;
	-webkit-animation: hasUpdataAnimate 1s;
	animation: hasUpdataAnimate 1s;
	-webkit-animation-fill-mode: forwards;
	animation-fill-mode: forwards;
}

@-webkit-keyframes hasUpdataAnimate {
	from {}
	to {
		opacity: 1
	}
}

@keyframes hasUpdataAnimate {
	from {}
	to {
		opacity: 1
	}
}



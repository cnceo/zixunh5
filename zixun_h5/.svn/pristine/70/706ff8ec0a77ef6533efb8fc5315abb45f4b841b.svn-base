	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	display: inline-block;
	position: relative;
	top: 0.02rem;
	float: left;
}
/* 加载动画*/

.loading {
    width: 100%;
    height: auto;
    position: absolute;
    left: 50%;
    top: 40%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}

.loading_bar {
    width: 30%;
    height: 0.10rem;
    background: url("/Static/images/v1.0/loading_bg.png") no-repeat center;
    background-size: 100% 100%;
    position: absolute;
    left: 50%;
    top: -0.3rem;
    -webkit-transform: translate(-50%, 0);
    -moz-transform: translate(-50%, 0);
    -ms-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
}

.loading_ball {
    width: 0.30rem;
    height: 0.30rem;
    background: url("/Static/images/v1.0/loading.png") no-repeat center;
    background-size: 100% 100%;
    position: absolute;
    left: 30%;
    top: -0.38rem;
    animation: myfirst 2s;
    -webkit-animation: myfirst 2s;
    /* Safari and Chrome */
    animation-timing-function: linear;
    -webkit-animation-timing-function: linear;
    animation-iteration-count: infinite;
    -webkit-animation-iteration-count: infinite;
    /*Safari and Chrome*/
}

.shuju {
    text-align: center;
}

@keyframes myfirst {
    from {
        left: 30%;
    }
    to {
        left: 60%;
    }
}

@-webkit-keyframes myfirst
/* Safari and Chrome */

{
    from {
        left: 30%;
    }
    to {
        left: 60%;
    }
}


/* 暂无数据*/

.no_data {
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 999999;
    padding-top: 1.3rem;
    background-color: #fff
}

.empty_data {
    position: absolute;
    top: 50%;
    width: 100%;
    height: 3rem;
    z-index: 9;
    background-color: #fff;
    -webkit-transform: translate(0, -50%);
    -moz-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
}

.no_img {
    background: url("/Static/images/v1.0/loaderror.png") no-repeat center;
    background-size: 0.83rem 1.20rem;
    height: 1.5rem;
    width: 100%
}

.no_tip {
    text-align: center;
    color: #7f7f7f
}

.no_tip p:first-child {
    font-size: 0.17rem
}

.re_load {
    width: 100%;

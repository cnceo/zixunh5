		<script type="text/javascript" charset="utf-8">
			$(function() {

				//mui.init();
				//窗体宽度发生变化
				window.onresize = function() {
					//背景图片 比例 667*375  	要根据
					$(".za-body-bg").css("height", document.documentElement.clientWidth * 667 / 375 + "px")
					$("#opemt").css("height", document.documentElement.clientWidth * 639 / 375 + "px")
					var bg_height = parseInt($(".za-body-bg").css("height").substring(0, $(".za-body-bg").css("height").length - 2));
					if(document.documentElement.clientHeight > bg_height) {
						$(".za-body-bg-more").css("height", document.documentElement.clientHeight - bg_height + "px")
					}
					$(".za-margin-top1").css("margin-top", bg_height * (100 / 667) + "px")
					//$(".za-margin-top2").css("margin-top",bg_height*(440/667)+"px")
					$(".za-body-bg img").css("width", bg_height * (99 / 667) + "px")
					$(".za-body-bg img").css("height", bg_height * (99 / 667) + "px")

					$("#img_1").css("margin-left", bg_height * (17 / 667) + "px")
					$("#img_1").css("margin-top", bg_height * (183 / 667) + "px")
					$("#img_11").css("margin-left", bg_height * (17 / 667) + "px")
					$("#img_11").css("margin-top", bg_height * (183 / 667) + "px")

					$("#img_0").css("margin-left", bg_height * (72 / 667) + "px")
					$("#img_0").css("margin-top", bg_height * (234 / 667) + "px")
					$("#img_00").css("margin-left", bg_height * (72 / 667) + "px")
					$("#img_00").css("margin-top", bg_height * (234 / 667) + "px")

					$("#img_2").css("margin-left", '-' + bg_height * (43 / 667) + "px")
					$("#img_2").css("margin-top", bg_height * (233 / 667) + "px")
					$("#img_22").css("margin-left", '-' + bg_height * (43 / 667) + "px")
					$("#img_22").css("margin-top", bg_height * (233 / 667) + "px")

					$("#img_3").css("margin-left", bg_height * (179 / 667) + "px")
					$("#img_3").css("margin-top", bg_height * (410 / 667) + "px")
					$("#img_33").css("margin-left", bg_height * (179 / 667) + "px")
					$("#img_33").css("margin-top", bg_height * (410 / 667) + "px")

					$("#img_4").css("margin-left", bg_height * (99 / 667) + "px")
					$("#img_4").css("margin-top", bg_height * (411 / 667) + "px")
					$("#img_44").css("margin-left", bg_height * (99 / 667) + "px")
					$("#img_44").css("margin-top", bg_height * (411 / 667) + "px")

					$("#img_rm").css("margin-left", bg_height * (137 / 667) + "px")
					$("#img_rm").css("margin-top", bg_height * (357 / 667) + "px")
				}

				//背景图片 比例 667*375  	要根据
				$(".za-body-bg").css("height", document.documentElement.clientWidth * 667 / 375 + "px")
				$("#opemt").css("height", document.documentElement.clientWidth * 639 / 375 + "px") //遮盖层 高度
				var bg_height = parseInt($(".za-body-bg").css("height").substring(0, $(".za-body-bg").css("height").length - 2));
				if(document.documentElement.clientHeight > bg_height) {
					$(".za-body-bg-more").css("height", document.documentElement.clientHeight - bg_height + "px")
				}
				$(".za-margin-top1").css("margin-top", bg_height * (100 / 667) + "px")
				$(".za-body-bg img").css("width", bg_height * (99 / 667) + "px")
				$(".za-body-bg img").css("height", bg_height * (99 / 667) + "px")

				$("#img_1").css("margin-left", bg_height * (17 / 667) + "px")
				$("#img_1").css("margin-top", bg_height * (183 / 667) + "px")
				$("#img_11").css("margin-left", bg_height * (17 / 667) + "px")
				$("#img_11").css("margin-top", bg_height * (183 / 667) + "px")

				$("#img_0").css("margin-left", bg_height * (72 / 667) + "px")
				$("#img_0").css("margin-top", bg_height * (234 / 667) + "px")
				$("#img_00").css("margin-left", bg_height * (72 / 667) + "px")
				$("#img_00").css("margin-top", bg_height * (234 / 667) + "px")

				$("#img_2").css("margin-left", '-' + bg_height * (43 / 667) + "px")
				$("#img_2").css("margin-top", bg_height * (233 / 667) + "px")
				$("#img_22").css("margin-left", '-' + bg_height * (43 / 667) + "px")
				$("#img_22").css("margin-top", bg_height * (233 / 667) + "px")

				$("#img_3").css("margin-left", bg_height * (179 / 667) + "px")
				$("#img_3").css("margin-top", bg_height * (410 / 667) + "px")
				$("#img_33").css("margin-left", bg_height * (179 / 667) + "px")
				$("#img_33").css("margin-top", bg_height * (410 / 667) + "px")

				$("#img_4").css("margin-left", bg_height * (99 / 667) + "px")
				$("#img_4").css("margin-top", bg_height * (411 / 667) + "px")
				$("#img_44").css("margin-left", bg_height * (99 / 667) + "px")
				$("#img_44").css("margin-top", bg_height * (411 / 667) + "px")

				$("#img_rm").css("margin-left", bg_height * (137 / 667) + "px")
				$("#img_rm").css("margin-top", bg_height * (357 / 667) + "px")

				//循环定时  动效
				var i = -1;
				var timer = window.setInterval(function() {
					i++;
					if(i == 5) {
						i = 0;
					}
					var id = "#img_" + i,
						id1 = "#img_" + i + i;
					for(var j = 0; j < 5; j++) {
						var hide = "#img_" + j,
							hide1 = "#img_" + j + j;
						$(hide).css({
							"display": "block"
						})
						$(hide1).css({
							"display": "none"
						})

					}
					$(id).css({
						"display": "none"
					})
					$(id1).css({
						"display": "block"
					})

				}, 1000);

				//足球菜单 点击
				$("#img_0").click(function() { //赛事推荐
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					$("#img_0").attr("src", "/Static/images/v1.0/worldcup/00.png?v=23e432423")
					window.clearTimeout(timer);
					setTimeout(function() {
						sessionStorage.cup_columnid = ZA_CUP_COLUMNID.sstj;
						window.location.href = "worldcup.html";
					}, 500)
				})
				$("#img_1").click(function() { //深度报道			
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					$("#img_1").attr("src", "/Static/images/v1.0/worldcup/11.png?v=23e432423")
					window.clearTimeout(timer);
					setTimeout(function() {
						sessionStorage.cup_columnid = ZA_CUP_COLUMNID.sdbd;
						window.location.href = "worldcup.html";
					}, 500)
				})
				$("#img_2").click(function() { //游戏活动
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					$("#img_2").attr("src", "/Static/images/v1.0/worldcup/22.png?v=23e432423")
					window.clearTimeout(timer);
					setTimeout(function() {
						sessionStorage.cup_columnid = ZA_CUP_COLUMNID.yxhd;
						window.location.href = "worldcup.html";
					}, 500)
				})
				$("#img_rm").click(function() { //热门
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					window.clearTimeout(timer);
					setTimeout(function() {
						sessionStorage.cup_columnid = ZA_CUP_COLUMNID.rm;
						window.location.href = "worldcup.html";
					}, 500)
				})
				$("#img_3").click(function() { //世界杯数据
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					$("#img_3").attr("src", "/Static/images/v1.0/worldcup/33.png?v=23e432423")
					window.clearTimeout(timer);
					setTimeout(function() {
						window.location.href = "record.html";
					}, 500)
				})
				$("#img_4").click(function() { //趣味世界杯
					$('img[id^="img_"]').each(function(index, item) {
						$("#img_" + index).attr("src", "/Static/images/v1.0/worldcup/" + index + "" + ".png?v=23e432423")
					})
					$("#img_4").attr("src", "/Static/images/v1.0/worldcup/44.png?v=23e432423")
					window.clearTimeout(timer);
					setTimeout(function() {
						sessionStorage.cup_columnid = ZA_CUP_COLUMNID.qwsjb;
						window.location.href = "worldcup.html";
					}, 500)
				})
				//分享图标点击
				$("#share-btn").click(function() {
					var config = {};
					config.title = document.title;
					config.url = window.location.href;
					config.desc = "为您提供有趣、有料的世界杯资讯和赛事推荐";
					config.img = "https:/Static/images/v1.0/worldcup/bg.png?v=23e432423";
                    //config.img = "/Static/images/v1.0/worldcup/bg.png?v=23e432423";
					var share_obj = new nativeShare(config);
					var modalBool = share_obj.bool;
					if(modalBool) {
						//layer.open({content:'浏览器暂不支持分享',skin:'msg',time:3})	
						if(isWeiXin()) {
							$("#opemt1").show();
						} else {
							$("#opemt").show();
						}
						
					} else {
						share_obj.share("weixin")
					}
				})

				//遮盖层点击
				$("#opemt").click(function() {
					$("#opemt").hide()
				})
				$("#opemt1").click(function() {
					$("#opemt1").hide()
				})
				//底部菜单点击
				$(".mui-bar-tab a").click(function(e) {
					var index = $(this).index();
					switch(index) {
						case 0:
							window.location.href = "../info/zixun/zixun.html";
							break;
						case 1:
							window.location.reload();
							break;
						case 2:
							window.location.href = "../info/zixun/zixun_live.html";
							break;
						case 3:
							window.location.href = "../user/user_index.html";
							break;
						default:
							break;
					}
				})
				//判断是否为微信内置浏览器
				function isWeiXin() {
					var ua = window.navigator.userAgent.toLowerCase();
					console.log(ua); //mozilla/5.0 (iphone; cpu iphone os 9_1 like mac os x) applewebkit/601.1.46 (khtml, like gecko)version/9.0 mobile/13b143 safari/601.1
					if(ua.match(/MicroMessenger/i) == 'micromessenger') {
						return true;
					} else {
						return false;
					}
				}
			})
		</script>
	</body>

